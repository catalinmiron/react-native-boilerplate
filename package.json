{
  "name": "RNBoilerplate",
  "version": "0.4.0",
  "scripts": {
    "remotedev": "remotedev-debugger --hostname localhost --port 5678 --runserver",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "ios-open": "open ios/*.xcodeproj",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios",
    "android": "node node_modules/react-native/local-cli/cli.js run-android",
    "lint": "eslint src test",
    "test": "mocha --recursive --compilers js:babel-core/register -r babel-polyfill -r test/setup",
    "cover": "babel-node node_modules/.bin/isparta cover _mocha -- --recursive -r test/setup",
    "rm-rnw-fbjs": "rm -rf node_modules/react-native-web/node_modules/fbjs",
    "postinstall": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json && rnpm link && npm run rm-rnw-fbjs",
    "newproject": "./newproject.sh"
  },
  "dependencies": {
    "immutable": "^3.7.6",
    "lodash": "^4.3.0",
    "react-native": "^0.21.0",
    "react-redux": "^4.4.0",
    "redux": "^3.3.1",
    "redux-thunk": "^1.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-eslint": "^5.0.0",
    "babel-polyfill": "^6.5.0",
    "babel-preset-react-native": "^1.4.0",
    "chai": "^3.5.0",
    "enzyme": "^2.0.0",
    "eslint": "^2.2.0",
    "eslint-config-airbnb": "^6.0.2",
    "eslint-plugin-react": "^4.1.0",
    "fbjs-scripts": "^0.5.0",
    "isparta": "^4.0.0",
    "mocha": "^2.4.5",
    "mockery": "^1.4.0",
    "node-fetch": "^1.3.3",
    "proxyquire": "^1.7.4",
    "react": "0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-native-web": "0.0.16",
    "remote-redux-devtools": "^0.1.2",
    "remote-redux-devtools-on-debugger": "^0.2.6",
    "replace": "^0.3.0",
    "rnpm": "^1.4.1",
    "sinon": "^1.17.2"
  },
  "devEngines": {
    "node": ">= 5.x",
    "npm": ">= 3.x"
  }
}
